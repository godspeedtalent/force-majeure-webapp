{
  "name": "force-majeure-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config config/vite.config.ts",
    "dev:full": "(supabase start || echo 'Supabase already running or failed to start') && npm run dev",
    "dev:migrate": "supabase db reset && npm run dev",
    "build": "vite build --config config/vite.config.ts",
    "build:dev": "vite build --mode development --config config/vite.config.ts",
    "lint": "eslint . --config config/eslint.config.js --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --config config/eslint.config.js --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "preview": "vite preview --config config/vite.config.ts",
    "test": "vitest --config config/vitest.config.ts",
    "test:ui": "vitest --config config/vitest.config.ts --ui",
    "test:coverage": "vitest --config config/vitest.config.ts --coverage",
    "test:watch": "vitest --config config/vitest.config.ts --watch",
    "upload-assets": "node scripts/upload-assets.js",
    "audit:design-system": "npx tsx scripts/audit-design-system.ts",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:db:reset": "supabase db reset",
    "supabase:migration:new": "supabase migration new",
    "supabase:studio": "supabase studio"

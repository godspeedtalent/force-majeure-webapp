# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_PROJECT_ID="your_project_id_here"
VITE_SUPABASE_PUBLISHABLE_KEY="your_publishable_key_here"
VITE_SUPABASE_URL="your_supabase_url_here"

# Spotify Configuration
SPOTIFY_CLIENT_ID="your_spotify_client_id_here"
SPOTIFY_CLIENT_SECRET="your_spotify_client_secret_here"
SPOTIFY_REDIRECT_URI="http://localhost:3000/callback"

# Delphi API Configuration (Service Authentication)
# Note: Store as Supabase secrets in production
DELPHI_API_BASE_URL="https://api.delphi.app"
DELPHI_API_KEY="your_delphi_api_key_here"
DELPHI_SECRET_KEY="your_delphi_secret_key_here"

# Optional: Development settings
# NODE_ENV="development"
# VITE_API_BASE_URL="http://localhost:3000"
